  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">script</span></span></span><span>&gt;<span class="language-javascript">
    </span></span><span><span class="hljs-keyword">async</span></span><span> </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">checkBattery</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
      </span><span><span class="hljs-keyword">if</span></span><span> (!(</span><span><span class="hljs-string">'getBattery'</span></span><span> </span><span><span class="hljs-keyword">in</span></span><span> navigator)) {
